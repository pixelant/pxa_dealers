<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.sheet_title</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <switchableControllerActions>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.actions.chooseMode</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.actions.map</numIndex>
                                        <numIndex index="1">Dealers->map</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.actions.categoriesFilter</numIndex>
                                        <numIndex index="1">Dealers->categoriesFilter</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.actions.countriesFilter</numIndex>
                                        <numIndex index="1">Dealers->countriesFilter</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.actions.categoriesCollectionFilter</numIndex>
                                        <numIndex index="1">Dealers->categoriesCollectionFilter</numIndex>
                                    </numIndex>
                                    <numIndex index="5">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.actions.search</numIndex>
                                        <numIndex index="1">Dealers->search</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </switchableControllerActions>

                    <settings.search.searchResultPage>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.search.searchResultPage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.search.searchResultPage>

                    <settings.filter.mapContentElement>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.mapContentElement</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <foreign_table>tt_content</foreign_table>
                                <foreign_table_where>AND tt_content.pid=###CURRENT_PID### AND tt_content.uid!=###THIS_UID### AND tt_content.list_type='pxadealers_pxadealers'</foreign_table_where>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                            </config>
                        </TCEforms>
                    </settings.filter.mapContentElement>

                    <settings.filter.categoriesFilterOptions>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.filter.categoriesFilterOptions</label>
                            <config>
                                <type>select</type>
                                <foreign_table>tx_pxadealers_domain_model_categoriesfilteroption</foreign_table>
                                <foreign_table_where> AND tx_pxadealers_domain_model_categoriesfilteroption.sys_language_uid IN (-1, 0)</foreign_table_where>
                                <size>5</size>
                                <minitems>1</minitems>
                                <maxitems>999</maxitems>
                            </config>
                        </TCEforms>
                    </settings.filter.categoriesFilterOptions>

                    <settings.demand.orderBy>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.orderBy</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <items>
                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.orderBy.name</numIndex>
                                        <numIndex index="1">name</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.orderBy.crdate</numIndex>
                                        <numIndex index="1">crdate</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.orderBy.tstamp</numIndex>
                                        <numIndex index="1">tstamp</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.demand.orderBy>

                    <settings.demand.orderDirection>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.orderDirection</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <items>
                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.orderDirection.asc</numIndex>
                                        <numIndex index="1">asc</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.orderDirection.asc Descending</numIndex>
                                        <numIndex index="1">desc</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.demand.orderDirection>

                    <settings.demand.countries>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.countries</label>
                            <config>
                                <type>select</type>
                                <size>10</size>
                                <renderType>selectMultipleSideBySide</renderType>
                                <foreign_table>static_countries</foreign_table>
                                <foreign_table_where>ORDER BY static_countries.cn_short_en</foreign_table_where>
                                <autoSizeMax>30</autoSizeMax>
                                <maxitems>99</maxitems>
                                <minitems>0</minitems>
                                <multiple>0</multiple>
                                <wizards>
                                    <_POSITION>top</_POSITION>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.demand.countries>

                    <settings.demand.categories>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.demand.categories</label>
                            <config>
                                <type>select</type>
                                <autoSizeMax>50</autoSizeMax>
                                <foreign_table>sys_category</foreign_table>
                                <foreign_table_where> AND sys_category.sys_language_uid IN (-1, 0) ORDER BY sys_category.sorting ASC</foreign_table_where>
                                <maxitems>9999</maxitems>
                                <renderMode>tree</renderMode>
                                <size>10</size>
                                <treeConfig>
                                    <appearance>
                                        <expandAll>1</expandAll>
                                        <showHeader>1</showHeader>
                                    </appearance>
                                    <parentField>parent</parentField>
                                </treeConfig>
                            </config>
                        </TCEforms>
                    </settings.demand.categories>
                </el>
            </ROOT>
        </sDEF>
        <map>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.sheet_title_map</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.map.mapHeight>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.mapHeight</label>
                            <config>
                                <type>input</type>
                                <default>555</default>
                                <size>10</size>
                                <eval>int</eval>
                            </config>
                        </TCEforms>
                    </settings.map.mapHeight>

                    <settings.map.markerClusterer.enable>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.enableMarkerClusterer</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.map.markerClusterer.enable>

                    <settings.map.markerClusterer.maxZoom>
                        <TCEforms>
                            <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.markerClustererMaxZoom</label>
                            <config>
                                <type>input</type>
                                <default>12</default>
                                <size>10</size>
                                <eval>int,required</eval>
                            </config>
                        </TCEforms>
                    </settings.map.markerClusterer.maxZoom>
                </el>
            </ROOT>
        </map>
    </sheets>
</T3DataStructure>