<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
  <sheets>
    <main>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.sheet_title</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>

          <settings.resultLimit>
                <TCEforms>
                  <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.resultLimit</label>
                  <config>
                    <type>input</type>
                    <default>12</default>
                    <size>10</size>
                    <eval>int</eval>
                  </config>
                </TCEforms>
          </settings.resultLimit>

          <!--<settings.newRow>-->
                <!--<TCEforms>-->
                  <!--<label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.newRow</label>-->
                  <!--<disabled>1</disabled>-->
                  <!--<config>-->
                    <!--<type>input</type>-->
                    <!--<size>10</size>-->
                    <!--<eval>int</eval>           -->
                  <!--</config>-->
                <!--</TCEforms>-->
          <!--</settings.newRow>-->

          <settings.additionalWrapperClass>
                <TCEforms>
                  <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.additionalWrapperClass</label>
                  <config>
                    <type>input</type>
                    <size>30</size>
                    <eval>trim</eval>           
                  </config>
                </TCEforms>
          </settings.additionalWrapperClass>

          <settings.additionalListWrapperClass>
                <TCEforms>
                  <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.additionalListWrapperClass</label>
                  <config>
                    <type>input</type>
                    <size>30</size>
                    <eval>trim</eval>           
                  </config>
                </TCEforms>
          </settings.additionalListWrapperClass>

          <settings.elementAdditionalClass>
                <TCEforms>
                  <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.elementAdditionalClass</label>
                  <config>
                    <type>input</type>
                    <size>30</size>
                    <eval>trim</eval>
                  </config>
                </TCEforms>
          </settings.elementAdditionalClass>

          <settings.clusterMarkers>
            <TCEforms>
              <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.clusterMarkers</label>
                <config>
                  <type>check</type>
                </config>
            </TCEforms>
          </settings.clusterMarkers>

          <settings.showLogo>
            <TCEforms>
              <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.showLogo</label>
                <config>
                  <type>check</type>
                </config>
            </TCEforms>
          </settings.showLogo>

          <settings.dealerLogoWidth>
            <TCEforms>
              <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.dealerLogoWidth</label>
              <displayCond>FIELD:settings.showLogo:=:1</displayCond>
                <config>
                    <type>input</type>
                    <size>30</size>
                    <eval>trim</eval>
                </config>
            </TCEforms>
          </settings.dealerLogoWidth>

          <settings.dealerLogoHeight>
            <TCEforms>
              <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.dealerLogoHeight</label>
              <displayCond>FIELD:settings.showLogo:=:1</displayCond>
                <config>
                    <type>input</type>
                    <size>30</size>
                    <eval>trim</eval>
                </config>
            </TCEforms>
          </settings.dealerLogoHeight>

          <settings.mapZoomLevel>
            <TCEforms>
              <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.mapZoomLevel</label>
              <config>
                <type>input</type>
                <size>30</size>
                <eval>int</eval>
                <default>12</default>
              </config>
            </TCEforms>
          </settings.mapZoomLevel>

            <settings.defaultCountry>
                <TCEforms>
                    <label>Default country</label>
                    <config>
                        <type>input</type>
                        <size>30</size>
                        <eval>trim</eval>
                    </config>
                </TCEforms>
            </settings.defaultCountry>

          <settings.layoutType>
            <TCEforms>
              <label>Layout type</label>
              <config>
                <type>select</type>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">Grid</numIndex>
                    <numIndex index="1">grid</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">Alphabet</numIndex>
                    <numIndex index="1">alphabet</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </settings.layoutType>

          <settings.showMap>
            <TCEforms>
              <label>Show map</label>
              <onChange>reload</onChange>
              <config>
                <type>check</type>
                <default>1</default>
              </config>
            </TCEforms>
          </settings.showMap>

          <settings.mapHeight>
            <TCEforms>
              <label>LLL:EXT:pxa_dealers/Resources/Private/Language/locallang_db.xlf:flexform.mapHeight</label>
              <displayCond>FIELD:settings.showMap:=:1</displayCond>
              <config>
                <type>input</type>
                <default>555</default>
                <size>10</size>
                <eval>int</eval>
              </config>
            </TCEforms>
          </settings.mapHeight>

          <settings.allowedCountries>
            <TCEforms>
              <label>Allowed countries</label>
              <config>
                <type>select</type>
                <foreign_table>static_countries</foreign_table>
                <foreign_table_where> ORDER BY static_countries.cn_short_en</foreign_table_where>
                <size>10</size>
                <autoSizeMax>20</autoSizeMax>
                <minitems>0</minitems>
                <maxitems>9999</maxitems>
              </config>
            </TCEforms>
          </settings.allowedCountries>

                </el>
            </ROOT>
        </main>
    </sheets>
</T3DataStructure>